   "source": [
    "import numpy as np\n",
    "from PIL import Image\n",
    "\n",
    "# 1024x1024 크기의 배열 생성, 모든 값은 1로 설정\n",
    "image_data = np.ones((1024, 1024), dtype=np.uint8)\n",
    "\n",
    "# 배열을 이미지로 변환\n",
    "image = Image.fromarray(image_data)\n",
    "\n",
    "# 이미지 저장\n",
    "image.save('output.tif')\n",
    "\n",
    "print('주인님 다했습니다.')"
   ]
